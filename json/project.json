      "components": [
        {
          "title": "Page 1",
          "label": "Page 1",
          "type": "panel",
          "key": "page1",
          "input": false,
          "tableView": false,
          "components": [
            {
              "label": "HTML",
              "tag": "div",
              "attrs": [
